#biaya rata-rata perjalanan taksi (fare, tips, dan taxes) berdasarkan metode pembayaran pada tahun 2019
SELECT
  payment_type,
  AVG(fare) AS average_fare,
  AVG(tips) AS average_tips,
  AVG(tolls) AS average_tolls
FROM
  `bigquery-public-data.chicago_taxi_trips.taxi_trips`
WHERE
  EXTRACT(YEAR FROM trip_start_timestamp) = 2019
  AND fare IS NOT NULL
  AND tips IS NOT NULL
  AND tolls IS NOT NULL
  AND payment_type IS NOT NULL
GROUP BY
  payment_type
ORDER BY
  average_fare DESC;
